{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "no-undef":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "padded-blocks":"off",
        "no-trailing-spaces":"off",
        "lines-between-class-members":"off",
        "eol-last":"off"
    }
}
